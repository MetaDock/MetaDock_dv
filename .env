# Workflow Builder Configuration
WORKFLOW_TEMP_DIR=/tmp/workflow_temp
WORKFLOW_MAX_EXECUTION_TIME=3600
WORKFLOW_MAX_CONCURRENT_JOBS=5
WORKFLOW_LOG_LEVEL=info

# File Upload Configuration for Workflow
WORKFLOW_UPLOAD_MAX_SIZE=100MB
WORKFLOW_ALLOWED_FILE_TYPES=.fastq,.fq,.fasta,.fa,.sam,.bam,.txt,.csv,.json

# Workflow Execution Configuration
WORKFLOW_DEFAULT_TIMEOUT=300
WORKFLOW_RETRY_ATTEMPTS=3
WORKFLOW_RETRY_DELAY=5000

# Workflow Storage Configuration
WORKFLOW_SAVE_DIR=./workflows
WORKFLOW_BACKUP_ENABLED=true
WORKFLOW_BACKUP_RETENTION_DAYS=30

